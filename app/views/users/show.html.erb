<div class="container">
  <h1> Welcome: <%= @user.username %></h1>
    <div class="row">
      <div class="col-md-4 col-md-offset-4">
        <%= gravatar_for @user, size: 150 %>
      </div>
    </div>
    <h4> <%= @user.username %>'s articles</h4>
    <%= render 'articles/article', obj: @user.articles %>
</div>
